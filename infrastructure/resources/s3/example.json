{
	"acl": "private",
	"//policy": {
		"Version": "2012-10-17",
		"Statement": [
			{
				"Effect": "Allow",
				"Principal": { "AWS": "arn:aws:iam::000000000000:user/SampleUser" },
				"Action": "s3:GetObject",
				"Resource": "arn:aws:s3:::avatar/*"
			},
			{
				"Effect": "Allow",
				"Principal": { "AWS": "arn:aws:iam::000000000000:user/SampleUser" },
				"Action": "s3:ListBucket",
				"Resource": "arn:aws:s3:::avatar"
			}
		]
	},
	"public_access_block": {
		"block_public_acls": true,
		"block_public_policy": true,
		"ignore_public_acls": true,
		"restrict_public_buckets": true
	},
	"trigger": [
		{
			"lambda": "sample",
			"events": ["s3:ObjectCreated:*"],
			"filter_prefix": "examplePrefix/",
			"filter_suffix": ".jpg"
		}
	]
}
